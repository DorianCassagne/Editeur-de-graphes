<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane fx:id="mainAnchorPane" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controleur.Controleur">
   <children>
      <BorderPane prefHeight="231.0" prefWidth="266.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <top>
            <ButtonBar buttonMinWidth="60.0" maxHeight="1.7976931348623157E308" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="20.0" prefWidth="400.0">
              <buttons>
                  <Button fx:id="btnSommet" mnemonicParsing="false" onAction="#creationSommet" text="creer Sommet" />
                  <Button fx:id="btnArete" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0" mnemonicParsing="false" onAction="#creationArete" text="creer arete" />
              </buttons>
               <BorderPane.margin>
                  <Insets left="10.0" right="10.0" />
               </BorderPane.margin>
               <padding>
                  <Insets left="10.0" right="80.0" />
               </padding>
            </ButtonBar>
         </top>
         <center>
            <Pane fx:id="feuilleDessin" onMouseClicked="#dessinSommet" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </children>
</AnchorPane>
